{
  "format": "custom",
  "id": "work",
  "label": "Work",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "config": {
          "label": "UID"
        },
        "type": "UID"
      },
      "description": {
        "type": "StructuredText",
        "config": {
          "label": "Description",
          "placeholder": "",
          "allowTargetBlank": true,
          "multi": "paragraph,preformatted,heading1,heading2,heading3,heading4,heading5,heading6,strong,em,hyperlink,image,embed,list-item,o-list-item,rtl"
        }
      },
      "company": {
        "type": "Group",
        "config": {
          "label": "Company",
          "repeat": true,
          "fields": {
            "name": {
              "type": "Text",
              "config": {
                "label": "name",
                "placeholder": "Mekaverse"
              }
            },
            "website": {
              "type": "Link",
              "config": {
                "label": "website",
                "placeholder": "",
                "allowTargetBlank": true,
                "select": null
              }
            },
            "logo": {
              "type": "Image",
              "config": {
                "label": "logo",
                "constraint": {},
                "thumbnails": []
              }
            }
          }
        }
      },
      "duration": {
        "type": "Group",
        "config": {
          "label": "duration",
          "repeat": false,
          "fields": {
            "start": {
              "type": "Date",
              "config": {
                "label": "start",
                "placeholder": ""
              }
            },
            "end": {
              "type": "Date",
              "config": {
                "label": "end",
                "placeholder": ""
              }
            }
          }
        }
      },
      "tags": {
        "type": "Group",
        "config": {
          "label": "tags",
          "repeat": true,
          "fields": {
            "name": {
              "type": "Text",
              "config": {
                "label": "name",
                "placeholder": ""
              }
            }
          }
        }
      },
      "relatedPage": {
        "type": "Link",
        "config": {
          "label": "relatedPage",
          "select": "document",
          "customtypes": ["WorkDetail"]
        }
      }
    }
  }
}
